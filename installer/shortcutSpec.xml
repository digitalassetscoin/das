<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="DAS" location="startMenu"/>
	<shortcut
		name="Das Server"
		target="$INSTALL_PATH\das.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start DAS server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
	<shortcut
		name="Das Wallet"
		target="http://localhost:7876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch DAS wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>